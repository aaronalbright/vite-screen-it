{
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "globals": {
      "$1": true,
      "pageInfo": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-console": 0,
      "comma-dangle": 0,
      "semi": [0, "always"],
      "no-unused-vars": 1
    }
  }
  